	add x1, x0, x0
	addi x2, x0, 7
INIT:
	lw x3, 0x50(x1)
	beq x3, x2, FINAL
	add x4, x3, x1
	addi x1, x1, 1 
	bne x3, x2, INIT
FINAL:
	sw x4, 0x40(x0)

